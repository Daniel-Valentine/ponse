{
  "name": "ponse",
  "version": "3.1.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Module for work with requests and responses",
  "homepage": "http://github.com/coderaiser/ponse",
  "main": "lib/ponse.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/ponse.git"
  },
  "scripts": {
    "lint:lib": "eslint lib",
    "lint:bin": "eslint --rule 'no-console:0' bin",
    "lint:test": "eslint -c .eslintrc.test test  --no-eslintrc",
    "lint": "redrun lint:*",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "test": "tape test/*.js",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec"
  },
  "keywords": [
    "request",
    "response",
    "send",
    "parse"
  ],
  "bin": {
    "ponse": "bin/ponse.js"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "execon": "^1.2.0",
    "files-io": "^3.0.0",
    "itype": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.1.0",
    "eslint-plugin-node": "^8.0.0",
    "got": "^9.2.2",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "tape": "^4.9.1",
    "try-to-catch": "^1.0.2"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  }
}
